{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "shell_shebang",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "shebang_line"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "script_content"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "shebang_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 1,
                "content": {
                  "type": "STRING",
                  "value": "#!"
                }
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[ \\t]+"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "direct_interpreter"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "args"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 1,
                "content": {
                  "type": "STRING",
                  "value": "#!"
                }
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[ \\t]+"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "env_path"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[ \\t]+"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "env_flags"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[ \\t]+"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "env_interpreter"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "args"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        }
      ]
    },
    "direct_interpreter": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^ \\t\\n]*\\/(bash|zsh)"
        }
      }
    },
    "env_path": {
      "type": "PATTERN",
      "value": "[^ \\t\\n]*\\/env"
    },
    "env_flags": {
      "type": "PATTERN",
      "value": "-[^ \\t\\n]+"
    },
    "env_interpreter": {
      "type": "PATTERN",
      "value": "(bash|zsh)"
    },
    "args": {
      "type": "PATTERN",
      "value": "[ \\t]+[^\\n]*"
    },
    "script_content": {
      "type": "PATTERN",
      "value": "(.|\\n)+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}